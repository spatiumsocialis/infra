    "version": "0.2.0",
    "configurations": [

    {
        "name": "Run scoring consumer",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "${workspaceFolder}/proximity/cmd/app",
        "env": {
            "DB_PROVIDER": "sqlite3",
            "DB_CONNECTION_STRING": ":memory:",
            "DOCKERHOST": "192.168.1.59",
            "KAFKA_PEERS": "192.168.1.59:9092",
            "KAFKA_CGROUP": "proximity",
            "PORT": "8080"
        }
    },
    {
        "name": "Run circle app",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "${workspaceFolder}/circle/cmd/app",
        "env": {
            "DB_PROVIDER": "sqlite3",
            "DB_CONNECTION_STRING": ":memory:",
            "DOCKERHOST": "192.168.1.59",
            "KAFKA_PEERS": "192.168.1.59:9092",
            "KAFKA_CGROUP": "proximity",
            "PORT": "8080"
        }
    }]
}